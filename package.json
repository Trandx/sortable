{
  "name": "@trandx/sortable",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "author": "Trandx-code",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true,
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trandx/sortable.git"
  },
  "homepage": "https://github.com/Trandx/sortable/blob/production/README.md",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublish": "npm run build && npm version patch "
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  }
}
